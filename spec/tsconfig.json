{
    "compilerOptions": {
        "target": "es5",
        "module": "esnext",
         "moduleResolution": "node",

        "declaration": false,

        "strict": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,

        "skipLibCheck": true,
        "importHelpers": true,
        "jsx": "react",
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "emitDecoratorMetadata": true,
        "newLine": "LF"
    }
}